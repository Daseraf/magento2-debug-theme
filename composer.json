{
    "name": "daseraf/magento2-debug-theme",
    "description": "Magento 2 debug module theme",
    "type": "magento2-theme",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "require": {
        "magento/framework": "~101.0|~102.0|~103.0",
        "magento/module-backend": "~100.2|~101.0|~102.0"
    },
    "config": {
        "preferred-install": "source"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Daseraf\\DebugTheme\\": ""
        }
    }
}
